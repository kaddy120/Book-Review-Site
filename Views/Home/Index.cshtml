@model IEnumerable<Books.Models.Book>

@{
    ViewData["Title"] = "Index";
}
    <!DOCTYPE html>
<html>

<h2>Book List</h2>

 <div>@{ 
    foreach(var Book in Model)
    {
        <div>
         <a asp-action="BookDetails" asp-route-Isbn="@Book.Isbn">
            <img src="@Book.ThumbnailUrl" alt="Sample Photo" width="85" /></a>
            <span class="BookTitle">@Book.Title</span>
        </div>
    }
}
 </div>

</html>